<odoo>
  <data>

  <record id="crm_extensions_autocomplete" model="ir.ui.view">
    <field name="name">Autocomplete Fields</field>
    <field name="model">crm.lead</field>
    <field name="inherit_id" ref="crm.crm_case_form_view_leads"/>
    <field name="arch" type="xml">
      <field name="partner_id" position="attributes">
        <attribute name="groups"></attribute>
    </field>
    </field>
  </record>
  <record id="crm_extensions_reminder" model="ir.ui.view">
    <field name="name">Reminder Form Adjustement</field>
    <field name="model">crm.lead</field>
    <field name="inherit_id" ref="crm.crm_case_form_view_leads" />
    <field name="arch" type="xml">
      <page name="extra" position="after">
        <page name="reminder" string="Reminder">
          <group>
            <field name="reminder_date"/>
          </group>
        </page>
      </page>
    </field>
  </record>



