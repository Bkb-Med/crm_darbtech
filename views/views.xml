<odoo>
  <data>
  <record id="crm_darbtech_extensions" model="ir.ui.view">
    <field name="name">Autocomplete and reminder </field>
    <field name="model">crm.lead</field>
    <field name="inherit_id" ref="crm.crm_case_form_view_leads"/>
    <field name="arch" type="xml">
      <page name="extra" position="after">
        <page name="reminder" string="Reminder">
          <group>
            <field name="reminder_date" placeholder="Choose date..."/>
          </group>
        </page> 
      </page>
      <field name="partner_id" position="attributes">
        <attribute name="groups"></attribute>
    </field>
    </field>
  </record>
  </data>
</odoo>